# Copy to .env and set real values for production
# Values below are safe defaults for local dev

# Backend FastAPI
JWT_SECRET=devsecret
JWT_ALGO=HS256
# If you want to override CORS origins
ALLOW_ORIGINS=http://localhost,http://localhost:5173
# Optional DB URL (uncomment docker-compose to use)
# DATABASE_URL=sqlite:///./app.db

# OpenAI (if used by backend)
# OPENAI_API_KEY=

# Frontend build-time API URL
VITE_API_URL=http://localhost:8000
